    "# The infamous Ackermann Function\n",
    "# Complexity\n",
    "def ack(m,n):\n",
    "    if m==0:\n",
    "        ans = n + 1\n",
    "    else if n == 0:\n",
    "        ans = ack(m-1, 1)\n",
    "    else:\n",
    "        ans = ack(m-1, ack(m,n-1))\n",
    "    print(ans)\n",
    "    return ans\n"
   ]
